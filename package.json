{
	"name": "yandex-middle",
	"private": false,
	"version": "0.0.5",
	"type": "module",
	"engines": {
		"node": ">=12.0.0"
	},
	"scripts": {
		"dev": "vite",
		"dev:server": "nodemon -I --exec node --loader ts-node/esm ./src/server/server.ts",

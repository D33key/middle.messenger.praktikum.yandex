		"nodemon": "^3.1.0",
		"postcss": "^8.4.38",
		"postcss-cli": "^11.0.0",
		"stylelint": "^16.5.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.2.2",
		"vite": "^5.2.0"
	},
	"dependencies": {
		"express": "^4.19.2",
		"uuid": "^9.0.1",
		"vite-express": "^0.16.0"

    "global": "^4.4.0",
    "uuid": "^9.0.1",
    "vite-express": "^0.16.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@types/chai": "^4.3.16",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.12.7",
    "@types/uuid": "^9.0.8",
    "chai": "^5.1.1",
    "core-js": "^3.37.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.41.0",
    "eslint-kit": "^10.0.0",
